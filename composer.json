{
    "name": "proget-hq/phpstan-yii2",
    "description": "Yii2 extension for PHPStan",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Arkadiusz Kondas",
            "email": "arkadiusz.kondas@gmail.com"
        },
        {
            "name": "Marcin Michalski",
            "email": "marcin@michalski.dev"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "nikic/php-parser": "^4.1.0",
        "phpstan/phpstan": "^1"
    },
    "require-dev": {
        "ely/php-code-style": "^0.5",
        "ergebnis/composer-normalize": "^2.30",
        "friendsofphp/php-cs-fixer": "^3.13",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-phpunit": "^1",
        "phpunit/phpunit": "^9",
        "yiisoft/yii2": "^2.0.42"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "autoload": {
        "psr-4": {
            "Proget\\PHPStan\\Yii2\\": "src/"
        },
        "exclude-from-classmap": [
            "src/Stubs/*"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Proget\\Tests\\PHPStan\\Yii2\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "phpstan/extension-installer": true,
            "ergebnis/composer-normalize": true
        },
        "sort-packages": true
    },
    "extra": {
        "phpstan": {
            "includes": [
                "extension.neon"
            ]
        }
    }
}
